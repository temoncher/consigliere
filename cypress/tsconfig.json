{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "../node_modules", // needed if your tests import npm modules
    "paths": {
      "@shared/*": [
        "../src/app/shared/*"
      ],
      "src/*": [
        "../src/*"
      ],
      "@e2e/*": [
        "../cypress/*"
      ]
    },
    "types": [
      "cypress",
      "node"
    ],
  },
  "exclude": [],
  "include": [
    "../node_modules/cypress/types",
    "**/*.ts"
  ]
}